    "ga-lite": "^2.1.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.5.0",
    "preact": "^10.5.7"
  },
  "browserslist": [
    "since 2017-06"
  ],
  "staticFiles": {
    "staticPath": "static"
